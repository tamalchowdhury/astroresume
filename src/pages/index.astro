---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../config';
---

<!DOCTYPE html>
<html lang='en'>
  <head>
    <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
  </head>
  <body>
    <Header title={SITE_TITLE} />
    <>
      <div class='wrapper'>
        <h1>Tamal Chowdhury's Developer Advocate Resume</h1>
        <p>
          Hey, you just landed in my website. I'm currently looking for my next
          role in tech so please check out my resume below.
        </p>
        <p>
          <a
            href='https://docs.google.com/document/d/1jZ0OooyveHh_0cZ3MLxXTmue8G64F9u-rsdnpWYGDiw/edit?usp=sharing'
            target='_blank'>View the Resume on Google Drive</a
          >
        </p>

        <p>Enjoy your visit!</p>
      </div>

      <iframe
        src='https://docs.google.com/document/d/1jZ0OooyveHh_0cZ3MLxXTmue8G64F9u-rsdnpWYGDiw/edit?usp=sharing'
        frameborder='0'
        width='100%'
        height='800px'></iframe>
    </>
    <Footer />
  </body>
</html>
